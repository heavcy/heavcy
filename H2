public static double calculate(double S, double x,double error){
		S=0.5*(S+(x/S));
		if((S*S)-x>=error){
			calculate(S,x,error);
			return S;
		}
    if(x-(S*S)>=error){
			calculate(S,x,error);
			return S;
		}
		System.out.println(S);
		return S;
	}
	
	public static void main (String[]args){
		double x;
		double S;
		double error;
		x=26;
		S=x/2;
		error=0.0001;
		System.out.println(calculate(S,x,error));
	}
